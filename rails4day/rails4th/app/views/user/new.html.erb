<h2> 글쓰기 </h2>
<form  action="/user/create">
  <div class="form-group">
    <label>작성자</label>
    <input name= "username" type="text" class="form-control" placeholder="이름써">
  </div>
  <div class="form-group">
    <label>제목</label>
    <input name="title" type="text" class="form-control" placeholder="제목써">
  </div>
  <div class="form-group">
    <label>내용</label>
    <textarea name="content" class="form-control" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">끼요욧!</button>
</form>
